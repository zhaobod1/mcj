SELECT c.*,g.goods_name,g.goods_thumb,g.goods_id,c.goods_number,c.goods_price FROM `mcj`.`ecs_cart` AS c  LEFT JOIN `mcj`.`ecs_goods` AS g ON g.goods_id=c.goods_id  WHERE c.session_id = '91ee3c7149ece6339c5b7e84db5de373' AND c.user_id=0  AND rec_type = '0'

SELECT SUM(goods_number) AS number, SUM(goods_price * goods_number) AS amount FROM `mcj`.`ecs_cart` AS c  WHERE c.session_id = '91ee3c7149ece6339c5b7e84db5de373' AND c.user_id=0  AND rec_type = '0'

DELETE FROM `mcj`.`ecs_sessions` WHERE expiry < 1494550196

SELECT c.cat_id, c.cat_name, c.measure_unit, c.parent_id, c.is_show, c.show_in_nav, c.grade, c.sort_order, COUNT(s.cat_id) AS has_children FROM `mcj`.`ecs_category` AS c LEFT JOIN `mcj`.`ecs_category` AS s ON s.parent_id=c.cat_id WHERE c.is_virtual= '0' GROUP BY c.cat_id ORDER BY c.parent_id, c.sort_order ASC

