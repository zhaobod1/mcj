SELECT u.user_money,u.email, u.pay_points, u.user_rank, u.rank_points,  IFNULL(b.type_money, 0) AS user_bonus, u.last_login, u.last_ip FROM `mcj`.`ecs_users` AS u  LEFT JOIN `mcj`.`ecs_user_bonus` AS ub ON ub.user_id = u.user_id AND ub.used_time = 0  LEFT JOIN `mcj`.`ecs_bonus_type` AS b ON b.type_id = ub.bonus_type_id AND b.use_start_date <= '2017-05-12' AND b.use_end_date >= '2017-05-12'  WHERE u.user_id = '372'

SELECT rank_id, discount FROM `mcj`.`ecs_user_rank` WHERE special_rank = '0' AND min_points <= 0 AND max_points > 0

UPDATE `mcj`.`ecs_users` SET visit_count = visit_count + 1,  last_ip = '112.224.65.212', last_login = '1494548484' WHERE user_id = '372'

SELECT c.cat_id, c.cat_name, c.measure_unit, c.parent_id, c.is_show, c.show_in_nav, c.grade, c.sort_order, COUNT(s.cat_id) AS has_children FROM `mcj`.`ecs_category` AS c LEFT JOIN `mcj`.`ecs_category` AS s ON s.parent_id=c.cat_id WHERE c.is_virtual= '0' GROUP BY c.cat_id ORDER BY c.parent_id, c.sort_order ASC

