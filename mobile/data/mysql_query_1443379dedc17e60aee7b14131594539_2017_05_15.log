SELECT g.goods_id, g.goods_name, g.goods_thumb, g.goods_img, g.shop_price AS org_price, IFNULL(mp.user_price, g.shop_price * '1') AS shop_price, g.market_price, g.promote_price, g.promote_start_date, g.promote_end_date FROM `mcj`.`ecs_link_goods` lg LEFT JOIN `mcj`.`ecs_goods` AS g ON g.goods_id = lg.link_goods_id LEFT JOIN `mcj`.`ecs_member_price` AS mp ON mp.goods_id = g.goods_id AND mp.user_rank = '0' WHERE lg.goods_id = '302' AND g.is_on_sale = 1 AND g.is_alone_sale = 1 AND g.is_delete = 0 LIMIT 4

