SELECT COUNT(*) FROM `mcj`.`ecs_comment` AS c 
										 LEFT JOIN `mcj`.`ecs_shaidan` AS s ON c.rec_id=s.rec_id
										 WHERE c.id_value = '314' AND c.status = 1 AND c.comment_rank > 0 

SELECT c.*, u.headimg, s.shaidan_id, s.status AS shaidan_status,s.title,s.message FROM `mcj`.`ecs_comment` AS c 
				LEFT JOIN `mcj`.`ecs_users` AS u ON c.user_id=u.user_id
				LEFT JOIN `mcj`.`ecs_shaidan` AS s ON c.rec_id=s.rec_id
				WHERE c.id_value = '314' AND c.status = 1 AND c.comment_rank > 0  ORDER BY c.add_time DESC LIMIT 5

