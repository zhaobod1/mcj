SELECT c.cat_id, c.cat_name, c.measure_unit, c.parent_id, c.is_show, c.show_in_nav, c.grade, c.sort_order, COUNT(s.cat_id) AS has_children FROM `mcj`.`ecs_category` AS c LEFT JOIN `mcj`.`ecs_category` AS s ON s.parent_id=c.cat_id WHERE c.is_virtual= '0' GROUP BY c.cat_id ORDER BY c.parent_id, c.sort_order ASC

SELECT cat_id, COUNT(*) AS goods_num  FROM `mcj`.`ecs_goods` WHERE is_delete = 0 AND is_on_sale = 1  GROUP BY cat_id

SELECT gc.cat_id, COUNT(*) AS goods_num  FROM `mcj`.`ecs_goods_cat` AS gc , `mcj`.`ecs_goods` AS g  WHERE g.goods_id = gc.goods_id AND g.is_delete = 0 AND g.is_on_sale = 1  GROUP BY gc.cat_id

SELECT cat_name, keywords, cat_desc, style, grade, filter_attr, parent_id FROM `mcj`.`ecs_category` WHERE cat_id = '358'

SELECT b.brand_id, b.brand_name, COUNT(*) AS goods_num FROM `mcj`.`ecs_brand`AS b, `mcj`.`ecs_goods` AS g LEFT JOIN `mcj`.`ecs_goods_cat` AS gc ON g.goods_id = gc.goods_id WHERE g.brand_id = b.brand_id AND (g.cat_id  IN ('358','387','388','385','386')  OR gc.cat_id  IN ('358','387','388','385','386') ) AND b.is_show = 1  AND g.is_on_sale = 1 AND g.is_alone_sale = 1 AND g.is_delete = 0 GROUP BY b.brand_id HAVING goods_num > 0 ORDER BY b.sort_order, b.brand_id ASC

