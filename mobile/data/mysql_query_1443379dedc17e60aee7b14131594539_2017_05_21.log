SELECT c.*,g.goods_name,g.goods_thumb,g.goods_id,c.goods_number,c.goods_price FROM `mcj`.`ecs_cart` AS c  LEFT JOIN `mcj`.`ecs_goods` AS g ON g.goods_id=c.goods_id  WHERE c.session_id = 'SESS_ID' AND c.user_id=0  AND rec_type = '0'

SELECT SUM(goods_number) AS number, SUM(goods_price * goods_number) AS amount FROM `mcj`.`ecs_cart` AS c  WHERE c.session_id = 'SESS_ID' AND c.user_id=0  AND rec_type = '0'

