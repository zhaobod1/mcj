SELECT g.goods_id, g.goods_name, g.market_price, g.shop_price AS org_price, g.promote_price, IFNULL(mp.user_price, g.shop_price * '1') AS shop_price, promote_start_date, promote_end_date, g.goods_brief, g.goods_thumb, goods_img, b.brand_name, g.is_best, g.is_new, g.is_hot, g.is_promote FROM `mcj`.`ecs_goods` AS g LEFT JOIN `mcj`.`ecs_brand` AS b ON b.brand_id = g.brand_id LEFT JOIN `mcj`.`ecs_member_price` AS mp ON mp.goods_id = g.goods_id AND mp.user_rank = '0' WHERE g.is_on_sale = 1 AND g.is_alone_sale = 1 AND g.is_delete = 0 AND g.brand_id = '68' AND (g.is_best = 1 OR (g.is_promote = 1 AND promote_start_date <= '1495668278' AND promote_end_date >= '1495668278')) ORDER BY g.sort_order, g.last_update DESC

