SELECT u.user_money,u.email, u.pay_points, u.user_rank, u.rank_points,  IFNULL(b.type_money, 0) AS user_bonus, u.last_login, u.last_ip FROM `mcj`.`ecs_users` AS u  LEFT JOIN `mcj`.`ecs_user_bonus` AS ub ON ub.user_id = u.user_id AND ub.used_time = 0  LEFT JOIN `mcj`.`ecs_bonus_type` AS b ON b.type_id = ub.bonus_type_id AND b.use_start_date <= '2017-05-30' AND b.use_end_date >= '2017-05-30'  WHERE u.user_id = '395'

