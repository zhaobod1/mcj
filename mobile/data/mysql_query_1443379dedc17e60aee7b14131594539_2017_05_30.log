SELECT gc.cat_id, COUNT(*) AS goods_num  FROM `mcj`.`ecs_goods_cat` AS gc , `mcj`.`ecs_goods` AS g  WHERE g.goods_id = gc.goods_id AND g.is_delete = 0 AND g.is_on_sale = 1  GROUP BY gc.cat_id

