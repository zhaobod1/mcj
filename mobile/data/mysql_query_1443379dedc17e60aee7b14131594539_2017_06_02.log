SELECT g.*, c.measure_unit, b.brand_id, b.brand_name AS goods_brand, m.type_money AS bonus_money, IFNULL(AVG(r.comment_rank), 0) AS comment_rank, IFNULL(mp.user_price, g.shop_price * '1') AS rank_price FROM `mcj`.`ecs_goods` AS g LEFT JOIN `mcj`.`ecs_category` AS c ON g.cat_id = c.cat_id LEFT JOIN `mcj`.`ecs_brand` AS b ON g.brand_id = b.brand_id LEFT JOIN `mcj`.`ecs_comment` AS r ON r.id_value = g.goods_id AND comment_type = 0 AND r.parent_id = 0 AND r.status = 1 LEFT JOIN `mcj`.`ecs_bonus_type` AS m ON g.bonus_type_id = m.type_id AND m.send_start_date <= '1496366313' AND m.send_end_date >= '1496366313' LEFT JOIN `mcj`.`ecs_member_price` AS mp ON mp.goods_id = g.goods_id AND mp.user_rank = '0' WHERE g.goods_id = '302' AND g.is_delete = 0 GROUP BY g.goods_id

