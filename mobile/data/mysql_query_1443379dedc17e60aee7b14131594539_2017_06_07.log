SELECT gb.*,g.*,gb.act_id AS group_buy_id, gb.goods_id, gb.ext_info, gb.goods_name, g.goods_thumb, g.goods_img FROM `mcj`.`ecs_goods_activity` AS gb, `mcj`.`ecs_goods` AS g WHERE gb.act_type = '1' AND g.goods_id = gb.goods_id AND gb.start_time <= '1496875919' AND gb.end_time >= '1496875919' AND g.is_delete = 0 ORDER BY gb.act_id DESC LIMIT 3

SELECT a.act_id, a.goods_id, a.goods_name, a.ext_info, g.goods_thumb FROM `mcj`.`ecs_goods_activity` AS a,`mcj`.`ecs_goods` AS g WHERE a.goods_id = g.goods_id AND a.act_type = '2' AND a.is_finished = 0 AND a.start_time <= '1496875919' AND a.end_time >= '1496875919' AND g.is_delete = 0 ORDER BY a.start_time DESC LIMIT 3

