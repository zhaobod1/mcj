SELECT pg.package_id, pg.goods_id, pg.goods_number, pg.admin_id, p.goods_attr, g.goods_sn, g.goods_name, g.market_price, g.goods_thumb, IFNULL(mp.user_price, g.shop_price * '1') AS rank_price
                FROM `mcj`.`ecs_package_goods` AS pg
                    LEFT JOIN `mcj`.`ecs_goods` AS g
                        ON g.goods_id = pg.goods_id
                    LEFT JOIN `mcj`.`ecs_products` AS p
                        ON p.product_id = pg.product_id
                    LEFT JOIN `mcj`.`ecs_member_price` AS mp
                        ON mp.goods_id = g.goods_id AND mp.user_rank = '0'
                WHERE pg.package_id = 4
                ORDER BY pg.package_id, pg.goods_id

